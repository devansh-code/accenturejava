import java.util.*;
public class AsciValue
{
    public static void main (String[] args) {
        Scanner sc=new Scanner(System.in);
        int []arr=new int[4];
            System.out.println("Enter the digits");
            for(int i=0;i<4;i++)
            {
                arr[i]=sc.nextInt();
            
            }
        for(int i=0;i<4;i++)
        {    int temp=arr[i];
            char c=(char)temp;
            System.out.printf("%d-%c\n",temp,c);
        }
    }
}
